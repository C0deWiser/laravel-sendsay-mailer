{
    "name": "codewiser/laravel-sendsay-mailer",
    "description": "SendSay mailer for Laravel",
    "keywords": ["Laravel", "Sendsay", "mailer", "mail"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "pm",
            "email": "pm@101media.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Codewiser\\LaravelSendSayMailer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codewiser\\LaravelSendSayMailer\\SendSayServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
